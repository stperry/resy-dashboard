{
  "name": "resy-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && npm run build-client && npm run build-server",
    "build-client": "webpack --config ./src/webpack.prod.ts",
    "build-server": "babel --out-dir=dist --ignore src/client/ --extensions=.tsx,.ts src",
    "clean": "rimraf build",
    "dev": "NODE_ENV=development babel-watch --extensions .ts src/server",
    "lint": "eslint --ext .js,.ts,.cjs,.mjs,.tsx,.jsx .",
    "lint-fix": "eslint --fix --ext .js,.ts,.cjs,.mjs,.tsx,.jsx .",
    "prod": "NODE_ENV=production node -r dotenv/config ./dist/server dotenv_config_path=\".env.production\"",
    "prod-build": "npm run build && npm run prod",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stperry/resy-dashboard.git"
  },
